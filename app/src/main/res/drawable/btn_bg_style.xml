<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true" >
        <shape>
            <corners android:radius="@dimen/Z10px" />
            <stroke android:width="1px" android:color="@color/default_color" />
            <solid android:color="#11ff8c00" />

        </shape>

    </item>

    <item>
        <shape>
            <solid android:color="@color/white"/>
            <corners android:radius="@dimen/Z10px" />
            <stroke android:width="1px" android:color="@color/default_color" />

        </shape>

    </item>

</selector>