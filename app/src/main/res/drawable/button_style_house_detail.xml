<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 边框颜色值 -->
    <item>
        <shape>
            <solid android:color="#FF9F00" />
        </shape>
    </item>
    <!-- 主体背景颜色值 -->
    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dip"
        android:top="1dip">
        <shape>
            <solid android:color="#ffffff" />
            <padding android:bottom="5dip" android:left="6dip" android:right="5dip" android:top="6dip"/>
        </shape>
    </item>
</layer-list>